{
  "version": 2,
  "build": {
    "env": {},
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "rootDirectory": "frontend"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/dist/index.html"
    }
  ]
}
